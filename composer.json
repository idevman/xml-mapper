{
    "name": "idevman/xml-mapper",
    "description": "Used to map XMLs to mapped arrays",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "xml",
        "mapping"
    ],
    "authors": [
        {
            "name": "devman",
            "email": "isc.sirius@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/support": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^4.4"
    },
    "autoload": {
        "psr-0": {
            "Idevman\\XmlMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idevman\\XmlMapper\\XmlMapperServiceProvider"
            ],
            "aliases": {
                "XmlMapper": "Idevman\\XmlMapper\\Support\\Facades\\XmlMapper"
            }
        }
    }
}
